{
  "name": "functional-fun-fair",
  "version": "2.0.0",
  "private": true,
  "description": "helping demystify functional programming",
  "scripts": {
    "test:once": "karma start karma.conf.js --single-run",
    "compile": "tsc -p src/",
    "build": "npm run compile && webpack --display-error-details",
    "postbuild": "npm run deploy",
    "predeploy": "rm -rf ../web-service/src/main/resources/webroot",
    "deploy": "cp -R dist ../web-service/src/main/resources/webroot",
    "prestart": "npm run compile && npm run build",
    "start": "npm run compile && webpack --display-error-details --watch",
    "watch": "npm run compile && webpack --display-error-details --watch",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "i18n": "ng-xi18n",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "update-deps": "npm update"
  },
